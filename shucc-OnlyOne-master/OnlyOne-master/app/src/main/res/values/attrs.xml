<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalProgressView">
        <attr name="horizontalColor" format="color" />
        <attr name="horizontalProgressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="android:layout_gravity" />
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <attr name="selectColor" format="color"/>
        <attr name="normalColor" format="color"/>
        <attr name="buttonColor" format="color"/>
        <attr name="buttonPadding" format="dimension" />
        <attr name="switchRate" format="integer" />
    </declare-styleable>
</resources>